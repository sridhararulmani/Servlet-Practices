<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
	<servlet-name>AddData</servlet-name>
	<servlet-class>com.servlet.jdbcDataStoringFromUI.AddData</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>AddData</servlet-name>
	<url-pattern>/Add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UpdateData</servlet-name>
		<servlet-class>com.servlet.jdbcDataStoringFromUI.UpdateData</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateData</servlet-name>
		<url-pattern>/UpDate</url-pattern>
	</servlet-mapping>
</web-app>